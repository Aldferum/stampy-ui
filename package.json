{
  "name": "stampy-ui",
  "description": "",
  "license": "MIT",
  "dependencies": {
    "@remix-run/cloudflare-workers": "^1.6.1",
    "@remix-run/react": "^1.6.1",
    "@tensorflow-models/universal-sentence-encoder": "^1.3.3",
    "@tensorflow/tfjs": "^3.18.0",
    "copy-to-clipboard": "^3.3.1",
    "lodash": "^4.17.21",
    "node-html-parser": "^5.3.3",
    "react": "^17.0.2",
    "react-auto-height": "^1.1.6",
    "react-dom": "^17.0.2",
    "remix": "^1.6.1",
    "seedrandom": "^3.0.5"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^2.2.2",
    "@remix-run/dev": "^1.6.1",
    "@types/lodash": "^4.14.182",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.16",
    "cross-env": "^7.0.3",
    "miniflare": "^2.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4",
    "wrangler": "^2.0.15"
  },
  "engines": {
    "node": ">=16"
  },
  "sideEffects": false,
  "scripts": {
    "postinstall": "remix setup cloudflare-workers",
    "lint": "prettier --write --ignore-path .gitignore .",
    "build": "rimraf public/build && cross-env NODE_ENV=production remix build",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare ./build/index.js --watch",
    "start": "cross-env NODE_ENV=development remix build && run-p dev:*",
    "dev": "npm start",
    "prod": "cross-env NODE_ENV=production miniflare ./build/index.js",
    "deploy": "npm run build && wrangler publish"
  },
  "main": "build/index.js"
}
